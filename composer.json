{
    "name": "laravel-notification-channels/rocket-chat",
    "description": "Rocket.Chat Notifications channel for Laravel 5.6+",
    "homepage": "https://github.com/laravel-notification-channels/rocket-chat",
    "license": "MIT",
    "keywords": [
        "laravel",
        "notifications",
        "notification-channel",
        "rocketchat",
        "rocket.chat",
        "chat"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "https://komarev.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.0|^8.0",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "illuminate/notifications": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/queue": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\RocketChat\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\RocketChat\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\RocketChat\\RocketChatServiceProvider"
            ]
        }
    }
}
